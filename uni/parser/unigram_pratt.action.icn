procedure arglist_action_3(items[])
    local   result,                     #LV: this is the result of the semantic
                                        #:   action that will be returned to the
                                        #:   parsing action
            restitem                    #LV: the lastitem of the parmlist that will
                                        #:   hold all remaining parameters

    restitem := ::pull(items[1].children)
    ::put(restitem.children, node("rest([])"))
    ::put(items[1].children, restitem)
    result :=  items[1]
    return result
end

