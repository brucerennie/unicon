UNI=$(BASE)/uni
BIN=$(BASE)/bin
RM=rm -f
RMDIR=rm -rf
MKDIR=mkdir
CP=cp -f
TOUCH=touch
ARC=zip
ARCEXT=zip

UNICON=$(BIN)/unicon
UC=$(UNICON)
